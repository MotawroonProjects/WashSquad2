                            android:longClickable="false"
                            android:padding="8dp"
                            android:rotation='@{lang.equals("en")?180:0}'
                            android:src="@drawable/ic_arrow_left"
                            app:tint="@color/colorPrimary" />
                    </LinearLayout>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
